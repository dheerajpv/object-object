{
    "compilerOptions": {
        "target": "es5",
        "module": "commonjs",
        "allowJs": true,
        "declaration": true,
        "outDir": "./dist",
        "importHelpers": true,
        "downlevelIteration": true,
        "strict": false,
        "noImplicitAny": true,
        "alwaysStrict": true,
        "moduleResolution": "node",
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true
    }
}
